<launch>
    <rosparam file="$(find cwru_davinci_control)/config/dvrk_psm_joint_state.yaml" command="load"/>

    <node name="joint_controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="/dvrk_psm" args="joint_state_controller" />

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
        <remap from="/joint_states" to="/dvrk_psm/joint_states" />
    </node>
</launch>